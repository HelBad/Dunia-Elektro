<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus.duniaelektro">

    <application
        android:allowBackup="true"
        android:icon="@drawable/duniaelektro"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Start"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Utama" />
        <activity
            android:name=".Resistor"
            android:label="Dunia Elektro"
            android:parentActivityName=".Utama" />
        <activity
            android:name=".Kapasitor"
            android:label="Dunia Elektro"
            android:parentActivityName=".Utama" />
        <activity
            android:name=".Transistor"
            android:label="Dunia Elektro"
            android:parentActivityName=".Utama" />
        <activity
            android:name=".Induktor"
            android:label="Dunia Elektro"
            android:parentActivityName=".Utama" />
        <activity
            android:name=".Rangkaian"
            android:label="Dunia Elektro"
            android:parentActivityName=".Utama" />
        <activity android:name=".ResistorRangkaian"
            android:label="Dunia Elektro"
            android:parentActivityName=".Resistor" />
        <activity android:name=".KapasitorRangkaian"
            android:label="Dunia Elektro"
            android:parentActivityName=".Kapasitor" />
        <activity android:name=".TransistorRangkaian"
            android:label="Dunia Elektro"
            android:parentActivityName=".Transistor" />
        <activity android:name=".InduktorRangkaian"
            android:label="Dunia Elektro"
            android:parentActivityName=".Induktor" ></activity>
    </application>

</manifest>